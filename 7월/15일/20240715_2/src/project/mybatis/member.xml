<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.dto.MemberDTO">
	<select id="login" 
			parameterType="dto"
			resultType="dto">
			
			select * from member
			where id = #{id}
			and   passwd = #{passwd}			
	
	</select>
	
	<!--  동적 sql 쿼리 이용 
		조건에 따라서 where 태그를 이용하면 
		조건이 참일 경우에는 자동으로 where키워드를
		붙이고 실행한다. 
	 -->
			
	
	
	
	
	

</mapper>